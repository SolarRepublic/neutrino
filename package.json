{
  "name": "@solar-republic/neutrino",
  "version": "0.9.8",
  "description": "Ultra light-weight client library for Secret Network",
  "homepage": "https://starshell.net/",
  "repository": "github:SolarRepublic/neutrino",
  "license": "MIT",
  "author": {
    "name": "Blake Regalia",
    "email": "blake.regalia@gmail.com",
    "url": "https://blake-regalia.com/"
  },
  "type": "module",
  "main": "dist/cjs/main.js",
  "module": "dist/mjs/main.js",
  "types": "dist/mjs/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/main.js",
      "import": "./dist/mjs/main.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/cjs",
    "dist/mjs"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:mjs": "tsc -p tsconfig.mjs.json && npx tsc-esm-fix --target='dist/mjs'",
    "build:cjs": "tsc -p tsconfig.cjs.json && npx tsc-esm-fix --target='dist/cjs'",
    "build": "yarn build:mjs && yarn build:cjs",
    "preview": "js-beautify dist/main.mjs > dist/main-pretty.mjs",
    "test": "node --loader ts-node/esm",
    "test:all": "yarn test test/**.ts",
    "debug": "node --inspect-brk --loader ts-node/esm"
  },
  "devDependencies": {
    "@blake.regalia/belt": "^0.21.0",
    "@blake.regalia/eslint-config-elite": "^0.3.3",
    "@blake.regalia/tsconfig": "^0.1.2",
    "@cosmjs/amino": "^0.31.3",
    "@nfps.dev/rollup-plugin-microweb": "^0.3.5",
    "@noble/secp256k1": "^2.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-replace": "^5.0.4",
    "@solar-republic/contractor": "^0.5.0",
    "@solar-republic/cosmos-grpc": "^0.7.1",
    "@solar-republic/types": "^0.1.4",
    "@types/chai": "^4.3.9",
    "@types/node": "^20.8.9",
    "@types/web": "^0.0.119",
    "@typescript-eslint/parser": "^6.9.0",
    "chai": "^4.3.10",
    "chai-bites": "^0.1.2",
    "curve25519-js": "^0.0.4",
    "esformatter": "^0.11.3",
    "eslint": "^8.52.0",
    "isomorphic-ws": "^5.0.0",
    "js-beautify": "^1.14.9",
    "rollup": "^4.1.5",
    "rollup-plugin-ignore": "^1.0.10",
    "ts-node": "^10.9.1",
    "ts-toolbelt": "^9.6.0",
    "tsc-esm-fix": "^2.20.17",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "ws": "^8.14.2"
  },
  "dependencies": {
    "@blake.regalia/belt": "^0.21.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
