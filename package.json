    "@cosmjs/amino": "^0.30.1",
    "@nfps.dev/rollup-plugin-microweb": "^0.1.0",
    "@noble/secp256k1": "^2.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@types/chai": "^4.3.5",
    "@types/node": "^20.2.0",
    "@types/web": "^0.0.99",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "chai-bites": "^0.1.2",
    "curve25519-js": "^0.0.4",
