    "rollup-plugin-ignore": "^1.0.10",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@blake.regalia/belt": "^0.5.2",
    "@solar-republic/aes-128-siv-js": "^0.1.8"
