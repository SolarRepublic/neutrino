  "type": "commonjs",
  "main": "dist/cjs/main.js",
  "module": "dist/mjs/main.js",
  "types": "dist/mjs/main.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rm -rf dist/*",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "build": "yarn build:cjs && yarn build:mjs",
    "preview": "js-beautify dist/main.mjs > dist/main-pretty.mjs",
    "test": "node --loader ts-node/esm",
    "test:all": "yarn test test/**.ts",
    "debug": "node --inspect-brk --loader ts-node/esm"
  },
  "devDependencies": {
    "@blake.regalia/belt": "^0.5.2",
    "@blake.regalia/eslint-config-elite": "^0.2.4",
